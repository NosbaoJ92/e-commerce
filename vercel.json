{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      },
      "./img/*.png": {
        "runtime": "vercel-php@0.6.0"
      },
      "./img/*.jpg": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/(.*)",  "dest": "./img/*.png" },
      { "src": "/(.*)",  "dest": "./img/*.jpg" }
    ]
  }