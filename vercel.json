{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      },
      "api/**/*.png": {
        "runtime": "vercel-php@0.6.0"
      },
      "api/**/*.jpg": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/(.*)",  "dest": "/api/img/*.png" },
      { "src": "/(.*)",  "dest": "/api/img/*.jpg" }
    ]
  }